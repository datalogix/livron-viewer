    "pdfjs-dist": "^4.6.82"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@iconify/json": "^2.2.277",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@types/node": "^22.10.1",
    "changelogen": "^0.5.7",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "sass": "^1.81.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
