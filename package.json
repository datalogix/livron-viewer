{
  "name": "livron-viewer",
  "version": "0.0.1",
  "description": "Livron viewer",
  "repository": "datalogix/livron-viewer",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint",
    "release": "pnpm test && pnpm build && changelogen --release --push && pnpm publish",
    "test": "pnpm lint"
  },
  "dependencies": {
    "@unocss/reset": "^0.64.0",
    "ofetch": "^1.4.1",
    "pdfjs-dist": "^4.6.82",
    "unpdf": "^0.12.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@iconify/json": "^2.2.268",
    "@stylistic/eslint-plugin": "^2.10.1",
    "@types/node": "^22.9.0",
    "changelogen": "^0.5.7",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "sass": "^1.80.6",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "unocss": "^0.64.0",
    "vite": "^5.4.10",
    "vite-plugin-eslint2": "^5.0.1"
  },
  "packageManager": "pnpm@9.12.3"
