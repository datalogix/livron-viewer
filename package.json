{
  "name": "livron-viewer",
  "version": "0.0.1",
  "description": "Google Fonts Helper",
  "repository": "datalogix/livron-viewer",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.js .",
    "release": "pnpm test && pnpm build && changelogen --release --push && pnpm publish",
    "test": "pnpm lint"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.232",
    "@types/node": "^22.0.1",
    "changelogen": "^0.5.5",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "unocss": "^0.61.9",
    "vite": "^5.3.5"
  },
  "dependencies": {
    "@unocss/reset": "^0.61.9",
    "ofetch": "^1.3.4",
    "pdfjs-dist": "^4.3.136",
    "unpdf": "^0.11.0"
  },
  "packageManager": "pnpm@9.6.0"
}
